{
  "name": "clinei",
  "author": "Arth",
  "version": "1.0.0-major",
  "description": "clinei is a command line interface handler to make it easy to build cli programs",
  "types": "./types/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./types/index.d.ts"
    }
  },
  "files": [
    "/dist",
    "/types",
    "config.json"
  ],
  "scripts": {
    "test": "node ./test/cjs/index.js & node ./test/mjs/index.js",
    "prettier": "prettier --write . & prettier --check .",
    "build": "rm -rf dist & tsup & prettier --write . &  prettier --check . & eslint ./ --fix --format=pretty",
    "prepublishOnly": "npm run build & npm run prettier"
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "cli",
    "command",
    "line",
    "interface",
    "handler",
    "command-line",
    "command-line-interface",
    "args",
    "arguments",
    "arg",
    "argument",
    "command-line-arguments",
    "command-line-argument",
    "command-line-args",
    "command-line-arg",
    "cli-args",
    "cli-arg",
    "program",
    "program-commands",
    "cli-program"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/4i8/clinei.git"
  },
  "bugs": {
    "url": "https://github.com/4i8/clinei/issues"
  },
  "homepage": "https://github.com/4i8/clinei#readme",
  "license": "Apache-2.0",
  "dependencies": {
    "glob": "^8.0.3"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "eslint-formatter-pretty": "^4.1.0",
    "prettier": "^2.8.4",
    "tsup": "^6.6.3"
  }
}
